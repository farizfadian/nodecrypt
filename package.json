{
  "name": "nodecrypt-jasypt",
  "version": "1.0.0",
  "description": "Jasypt-like encryption library for Node.js - Encrypt your configuration with ENC(...) pattern",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "bin": {
    "nodecrypt": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "encryption",
    "jasypt",
    "configuration",
    "security",
    "aes",
    "spring-boot",
    "config-encryption",
    "crypto",
    "nodejs",
    "typescript"
  ],
  "author": {
    "name": "Fariz Fadian",
    "email": "farislash@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/farizfadian/nodecrypt.git"
  },
  "bugs": {
    "url": "https://github.com/farizfadian/nodecrypt/issues"
  },
  "homepage": "https://github.com/farizfadian/nodecrypt#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
